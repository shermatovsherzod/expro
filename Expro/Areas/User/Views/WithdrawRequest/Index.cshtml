@using Expro.ViewModels
@using Expro.Models.Enums

@{
    ViewData["Title"] = "Запросы на вывод средств";
    ViewData["Area"] = UserAreasEnum.User;
}

<h1>@ViewData["Title"]</h1>
<br />

@await Html.PartialAsync("/Views/Shared/WithdrawRequestPartials/_Index.cshtml")

<link href="~/lib/DataTables-1.10.22/datatables.css" rel="stylesheet" />

@section Scripts
{
    <script src="~/lib/DataTables-1.10.22/datatables.js"></script>
    <script src="~/js/Shared/WithdrawRequest/Index.js"></script>
    <script>
        SetVariables(
            "@ViewData["Area"]",
            @((int)WithdrawRequestStatusesEnum.Pending),
            @((int)WithdrawRequestStatusesEnum.Completed));
    </script>
}