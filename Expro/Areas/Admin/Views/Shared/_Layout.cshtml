                                            <!-- End Nav Link -->
                                            <!-- View More - Collapse -->
                                            <div class="collapse" id="collapseSectionTwo">
                                                <a class="dropdown-item d-flex justify-content-between align-items-center px-0" href="#">
                                                    Java Developer
                                                </a>
                                                <a class="dropdown-item d-flex justify-content-between align-items-center px-0" href="#">
                                                    React
                                                </a>
                                            </div>
                                            <!-- End View More - Collapse -->
