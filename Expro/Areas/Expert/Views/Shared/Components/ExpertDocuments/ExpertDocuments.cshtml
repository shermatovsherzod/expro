@model Expro.ViewModels.DocumentsCountVM;
@{
    ViewData["Title"] = "ExpertDocuments";
}

<h6 class="text-cap small">@localizer["Documents"]</h6>

<!-- List -->
<ul class="nav nav-sub nav-sm nav-tabs nav-list-y-2 mb-4">
    <li class="nav-item">
        <a class="nav-link " href="/Expert/ArticleDocument">
            <i class="fas fa-check nav-icon"></i> @localizer["MyArticles"]
            <span class="badge badge-soft-dark badge-pill nav-link-badge">@Model.ArticleDocumentsCount</span>
        </a>
    </li>
    <li class="nav-item">
        <a class="nav-link " href="/Expert/SampleDocument">
            <i class="fas fa-check nav-icon"></i> @localizer["SampleDocuments"]
            <span class="badge badge-soft-dark badge-pill nav-link-badge">@Model.SampleDocumentsCount</span>
        </a>
    </li>
    <li class="nav-item">
        <a class="nav-link " href="/Expert/PracticeDocument">
            <i class="fas fa-check nav-icon"></i> @localizer["PracticeDocuments"]
            <span class="badge badge-soft-dark badge-pill nav-link-badge">@Model.PracticeDocumentsCount</span>
        </a>
    </li>
</ul>
<!-- End List -->